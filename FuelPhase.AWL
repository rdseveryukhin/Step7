FUNCTION "TIM_S5TI" : S5TIME
TITLE = IEC Time to S5 Time
AUTHOR : SIMATIC
FAMILY : IEC
NAME : TIM_S5TI
STANDARD
VERSION : 1.2


VAR_INPUT
  IN : TIME ;	// T#hh:mm:ss.sss 
END_VAR
VAR_TEMP
  y_Base : BYTE ;	// Zeitbasis
  d_Temp : DWORD ;	
END_VAR
BEGIN
NETWORK
TITLE =

      SET   ; 
      SAVE  ; // BIE setzen
      L     0; // Zeitbasis löschen
      T     #y_Base; 
      L     #IN; 
      T     #d_Temp; 
      L     0; // Zeitwert negativ
      <D    ; // ja, dann S5_Time := 0
      JC    FEHL; 
      TAK   ; 
      L     L#9990000; // Zeitwert > als erlaubt
      <=D   ; 
      JC    LOP; 
      L     W#16#3999; // ja, S5_Time := 3.999
      JC    FEHL; 
LOP:  L     #d_Temp; // Zeit
      L     L#10; 
      /D    ; // Zeit : 10	
      T     #d_Temp; 
      L     L#999; // Ergebnis : > 999
      <=D   ; 
      JC    FERT; // ja, dann Basis + 1
      L     #y_Base; 
      INC   1; 
      T     #y_Base; 
      JU    LOP; 
FERT: TAK   ; // Zeitwert holen
      DTB   ; // Wandlung BCD
      L     #y_Base; // Basis einblenden
      SLW   12; 
      OW    ; 
      JU    RET; 
FEHL: CLR   ; // BIE := 0
      SAVE  ; 
RET:  T     #RET_VAL; 
      BE    ; 
END_FUNCTION

